{
  "name": "i嘉然",
  "description": "基于Asoul人物形象制作多功能谷歌插件",
  "version": "0.0.2",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "history",
    "pageCapture",
    "cookies",
    "tabs",
    "notifications",
    "webRequest",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "jia-ran",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo.png",
      "32": "/images/logo.png",
      "48": "/images/logo.png",
      "128": "/images/logo.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "/static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js",
        "scripts/jquery.min.js",
        "scripts/qrcode.min.js"
      ],
      "css": [
        "scripts/content.css"
      ]
    }
  ],
  "icons": {
    "16": "/images/logo.png",
    "32": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  }
}